# HelioGraph RAG - Environment Configuration
# Copy this file to .env and update values as needed

# Document Registry Service
REGISTRY_DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/heliograph
REGISTRY_DB_POOL_SIZE=5
REGISTRY_DB_MAX_OVERFLOW=10
REGISTRY_DB_ECHO=false

REGISTRY_SQS_QUEUE_URL=http://localhost:4566/000000000000/document-registered
REGISTRY_SQS_REGION=us-east-1
REGISTRY_SQS_ENDPOINT_URL=http://localhost:4566

REGISTRY_S3_BUCKET=heliograph-documents
REGISTRY_S3_REGION=us-east-1
REGISTRY_S3_ENDPOINT_URL=http://localhost:4566

REGISTRY_FUZZY_MATCH_THRESHOLD=0.9
REGISTRY_LOG_LEVEL=INFO
REGISTRY_LOG_JSON=false
REGISTRY_DEBUG=true

# Database (for local development)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=heliograph

# Neo4j
NEO4J_AUTH=neo4j/heliograph123

# Redis
REDIS_URL=redis://localhost:6379/0
